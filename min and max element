#include<stdio.h>
int main()
{
  int i,j,n,a[20],max,min=10000;
  printf("enter the size:");
  scanf("%d",&n);
  printf("enter the elements");
  for(i=0;i<n;i++)
  {
  	scanf("%d",&a[i]);
  }
  for(i=0;i<n;i++)
  {
  	if(a[i]>max)
  	{
  		max=a[i];
	  }
  
    if(a[i]<min)
   {
  	min=a[i];
   }
}
  printf("max=%d and min=%d",max,min);
	 
}
