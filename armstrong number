#include <stdio.h>
int main()
{
   int n,r,sum=0,c,temp;
   printf("enter any number:");
   scanf("%d",&n);
   temp=n;
   while (n>0)
   {
   	r=n%10;
   	c=r*r*r;
   	sum=sum+c;
   	n=n/10;
   }
   n=temp;
   if(n==sum)
   printf("ARMSTRONG NUMBER");
   else
   printf("NOT ARMSTRONG NUMBER");
   
}
